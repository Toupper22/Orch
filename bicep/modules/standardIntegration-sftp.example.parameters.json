{
  "$schema": "https://schema.management.azure.com/schemas/2019-04-01/deploymentParameters.json#",
  "contentVersion": "1.0.0.0",
  "parameters": {
    "prefix": {
      "value": "edmo"
    },
    "environment": {
      "value": "dev"
    },
    "location": {
      "value": "swedencentral"
    },
    "locationShort": {
      "value": "sdc"
    },
    "integrationName": {
      "value": "sftp-integration"
    },
    "tags": {
      "value": {
        "Project": "Integration Layer",
        "CostCenter": "IT"
      }
    },
    "storageFirewallDefault": {
      "value": "Allow"
    },
    "integrationStorageContainers": {
      "value": [
        {
          "name": "input-files",
          "publicAccess": "None"
        },
        {
          "name": "processed",
          "publicAccess": "None"
        },
        {
          "name": "errors",
          "publicAccess": "None"
        },
        {
          "name": "archive",
          "publicAccess": "None"
        }
      ]
    },
    "integrationStorageTables": {
      "value": [
        {
          "name": "ProcessingLog"
        },
        {
          "name": "ErrorLog"
        }
      ]
    },
    "integrationStorageInstance": {
      "value": "int"
    },
    "sftpHost": {
      "value": "sftp.example.com"
    },
    "sftpPort": {
      "value": 22
    },
    "sftpUsername": {
      "value": "integration-user"
    },
    "sftpPassword": {
      "value": ""
    },
    "sftpSshPrivateKey": {
      "value": ""
    },
    "sftpSshPrivateKeyPassphrase": {
      "value": ""
    },
    "sftpRootFolder": {
      "value": "/inbound"
    },
    "sftpAcceptAnySshHostKey": {
      "value": true
    }
  }
}
