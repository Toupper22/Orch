        {
          "name": "ar-transactions",
          "publicAccess": "None"
        },
        {
          "name": "payments",
          "publicAccess": "None"
        },
        {
          "name": "archive",
          "publicAccess": "None"
        }
      ]
    },
    "integrationStorageTables": {
      "value": [
        {
          "name": "Values"
        },
        {
          "name": "Conversions"
        }
      ]
    },
    "integrationStorageInstance": {
      "value": "int"
    },
    "functionAppSettings": {
      "value": []
    },
    "enableBlobApiConnection": {
      "value": false
    },
    "enableTableApiConnection": {
      "value": false
    }
  }
