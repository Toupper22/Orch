{
  "$schema": "https://schema.management.azure.com/schemas/2019-04-01/deploymentParameters.json#",
  "contentVersion": "1.0.0.0",
  "parameters": {
    "prefix": {
      "value": "edmo"
    },
    "environment": {
      "value": "dev"
    },
    "location": {
      "value": "swedencentral"
    },
    "locationShort": {
      "value": "sdc"
    },
    "integrationName": {
      "value": "sepa"
    },
    "tags": {
      "value": {
        "Environment": "Development",
        "Customer": "efimademo",
        "Project": "demoproject",
        "Integration": "SEPA Integration"
      }
    },
    "serviceBusSku": {
      "value": "Standard"
    },
    "serviceBusQueues": {
      "value": [
        {
          "name": "sepa-inbound",
          "maxDeliveryCount": 10,
          "lockDuration": "PT5M",
          "defaultMessageTimeToLive": "P7D"
        },
        {
          "name": "sepa-errors",
          "maxDeliveryCount": 10,
          "lockDuration": "PT5M",
          "defaultMessageTimeToLive": "P14D"
        }
      ]
    },
    "storageFirewallDefault": {
      "value": "Deny"
    },
    "functionStorageContainers": {
      "value": []
    },
    "functionStorageTables": {
      "value": []
    },
    "integrationStorageContainers": {
      "value": [
        {
          "name": "sepa",
          "publicAccess": "None"
        },
        {
          "name": "sepa-processed",
          "publicAccess": "None"
        },
        {
          "name": "sepa-transformed",
          "publicAccess": "None"
        },
        {
          "name": "sepa-errors",
          "publicAccess": "None"
        }
      ]
    },
    "integrationStorageTables": {
      "value": [
        {
          "name": "ErrorLog"
        },
        {
          "name": "ProcessingLog"
        }
      ]
    },
    "integrationStorageInstance": {
      "value": "arc"
    },
    "functionAppSettings": {
      "value": []
    },
    "enableBlobApiConnection": {
      "value": true
    },
    "enableTableApiConnection": {
      "value": true
    }
  }
}
