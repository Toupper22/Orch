{
  "$schema": "https://schema.management.azure.com/schemas/2019-04-01/deploymentParameters.json#",
  "contentVersion": "1.0.0.0",
  "parameters": {
    "prefix": {
      "value": "customer"
    },
    "environment": {
      "value": "uat"
    },
    "location": {
      "value": "westeurope"
    },
    "locationShort": {
      "value": "weu"
    },
    "tags": {
      "value": {
        "Environment": "UAT",
        "Customer": "CUSTOMER_NAME_HERE",
        "Project": "PROJECT_NAME_HERE",
        "CostCenter": "IT-UAT"
      }
    },
    "deployKeyVault": {
      "value": true
    },
    "deployStorageAccount": {
      "value": true
    },
    "deployAppServicePlan": {
      "value": true
    },
    "deployManagedIdentity": {
      "value": true
    },
    "keyVaultSku": {
      "value": "standard"
    },
    "keyVaultSoftDeleteRetentionInDays": {
      "value": 90
    },
    "storageAccountSku": {
      "value": "Standard_LRS"
    },
    "storageContainers": {
      "value": [{"name":"integration-files","publicAccess":"None"},{"name":"logs","publicAccess":"None"}]
    },
    "appServicePlanSku": {
      "value": "B1"
    },
    "appServicePlanKind": {
      "value": "app"
    },
    "enableDiagnostics": {
      "value": true
    },
    "logAnalyticsWorkspaceId": {
      "value": ""
    },
    "deployVirtualNetwork": {
      "value": true
    },
    "deployNatGateway": {
      "value": true
    },
    "vnetAddressPrefixes": {
      "value": ["10.2.0.0/16"]
    },
    "subnets": {
      "value": [
  {
    "name": "integration-subnet",
    "addressPrefix": "10.2.1.0/24",
    "description": "Subnet for integration resources (Function Apps, Logic Apps)"
  },
  {
    "name": "private-endpoint-subnet",
    "addressPrefix": "10.2.2.0/24",
    "description": "Subnet for private endpoints"
  }
]
    },
    "natGatewayIdleTimeoutInMinutes": {
      "value": 4
    },
    "deployApplicationInsights": {
      "value": true
    },
    "applicationInsightsRetentionInDays": {
      "value": 90
    },
    "applicationInsightsDailyDataCapInGB": {
      "value": 10
    },
    "alertEmailReceivers": {
      "value": [
        {
          "name": "Admin",
          "emailAddress": "kalle.korhonen@efima.com",
          "useCommonAlertSchema": true
        }
      ]
    },
    "enableDefaultAlerts": {
      "value": true
    }
  }
}
